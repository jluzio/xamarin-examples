<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GenericDev.ImageExamples.ExamplesPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Loading Indicator" Clicked="OnLoadingIndicatorButtonClick" />
            <Button Text="Uri Image" Clicked="OnUriImageButtonClick" />
            <Button Text="Embedded Image" Clicked="OnEmbeddedImageButtonClick" />
            <Button Text="Local Image" Clicked="OnLocalImageButtonClick" />
            <Button Text="Image Circle Plugin" Clicked="OnImageCirclePluginButtonClick" />
            <Button Text="Exercise 1" Clicked="OnExercise1ButtonClick" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>